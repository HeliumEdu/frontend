{
    "name": "helium-frontend",
  "version": "1.12.7",
    "description": "Frontend for Helium Student Planner",
    "author": "Alex Laird",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/HeliumEdu/frontend/issues"
    },
    "homepage": "https://github.com/HeliumEdu/frontend#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HeliumEdu/frontend.git"
    },
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "build-dev": "ENVIRONMENT=local ./bin/build",
        "build": "./bin/build",
        "dev-server": "make clean-assets && npm run build-dev && http-server ./build -p 3000 -a localhost -d false -c-1",
        "start": "nodemon --watch src --ext js,mjs,cjs,json,css,html --exec npm run dev-server",
        "test": "npm run build-dev && npm run build"
    },
    "devDependencies": {
        "http-server": "^14.1.1",
        "nodemon": "^3.1.10"
    },
    "dependencies": {
        "chokidar": "^4.0.3",
        "clean-css-cli": "^4.3.0",
        "nunjucks-cli": "^1.0.0",
        "uglify-js": "^3.13.10"
    }
}
